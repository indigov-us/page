{
  "env": {
    "development": {
      "presets": [
        ["next/babel", {
          "styled-jsx": {
            "plugins": [
              "styled-jsx-plugin-postcss"
            ]
          }
        }]
      ]
    },
    "test": {
      "presets": [
        ["env", {"modules": "commonjs"}],
        ["next/babel", {
          "styled-jsx": {
            "plugins": [
              "styled-jsx-plugin-postcss"
            ]
          }
        }]
      ]
    },
    "production": {
      "presets": [
        ["next/babel", {
          "styled-jsx": {
            "plugins": [
              "styled-jsx-plugin-postcss"
            ]
          }
        }]
      ]
    }
  }
}
